[proxy]
%{ for index, proxy in proxys ~}
${ proxys_ids[index]} ansible_host=${ proxy } internal_ip=${ proxy_internal_ips[index] }
%{ endfor ~}

[proxy:vars]
ansible_user=${proxys_user}
ansible_private_key_file=${proxys_priv_key_path}

[master]
%{ for index, master in masters ~}
${ masters_ids[index]} ansible_host=${ master } internal_ip=${ master_internal_ips[index] }
%{ endfor ~}

[master:vars]
ansible_user=${masters_user}
ansible_private_key_file=${masters_priv_key_path}

[workers]
%{ for index, worker in workers ~}
${ workers_ids[index]} ansible_host=${ worker } internal_ip=${ workers_internal_ips[index] }
%{ endfor ~}

[workers:vars]
ansible_user=${workers_user}
ansible_private_key_file=${workers_priv_key_path}

[ipfs]
%{ for index, ipfs_peer in ipfs_peers ~}
${ ipfs_peers_ids[index]} ansible_host=${ ipfs_peer } internal_ip=${ ipfs_peers_internal_ips[index] }
%{ endfor ~}

[ipfs:vars]
ansible_user=${ipfs_peers_user}
ansible_private_key_file=${ipfs_peers_priv_key_path}